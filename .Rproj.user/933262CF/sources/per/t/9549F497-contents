for (symbol in target_stocks$Ticker){
url<-paste0("http://ichart.finance.yahoo.com/table.csv?s=",symbol,"&a=08&b=7&c=1984&d=01&e=23&f=2014&g=d&ignore=.csv")
stock<-read.csv(url)
